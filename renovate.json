{
  "groupName": "all-updates",
  "groupSlug": "all-updates",
  "packageRules": [
    {
      "matchUpdateTypes": [ "minor", "patch", "pin", "digest" ],
      "groupName": "all-updates"
    },
    {
      "matchPackageNames": [ "stefanzweifel/git-auto-commit-action" ],
      "enabled": false
    }
  ],
  "prConcurrentLimit": 1,
  "prHourlyLimit": 1,
  "schedule": [
    "before 7am every weekday"
  ]
}
